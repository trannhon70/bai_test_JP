{
  "name": "fontend-qlns",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist",
    "build": "rm -rf dist && NODE_OPTIONS='--max-old-space-size=4096' vite build",
    "start": "pm2 start npm --name vite-app -- run start:prod",
    "start:prod": "vite preview --port 3002",
    "restart": "fuser -k 3002/tcp || true && pm2 stop vite-app || true && pm2 delete vite-app || true && npm run clean && pm2 flush && npm run build && pm2 start npm --name vite-app -- run start:prod"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.15",
    "antd": "^5.24.4",
    "axios": "^1.8.4",
    "gsap": "^3.13.0",
    "jquery": "^3.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/file-saver": "^2.0.7",
    "@types/jquery": "^3.5.33",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "i": "^0.3.7",
    "npm": "^11.6.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.15",
    "vite": "^6.2.0"
  }
}
